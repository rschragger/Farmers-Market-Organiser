
<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">Stall</th>
      <th scope="col">Description</th>
      <th scope="col">Price</th>
      <th scope="col">Location</th>
    </tr>
  </thead>
  <tbody>
  {{#each stalls as |stall|}}
    <tr>
      <td>{{stall_name}}</td>
      <td>{{description}}</td>
      <td>{{price}}</td>
      <td>{{location.market_name}}</td>
    </tr>
   {{!-- {{> stall-details}} --}}
  {{/each}}
  </tbody>
</table>
